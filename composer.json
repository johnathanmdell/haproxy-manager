{
  "name": "merchnt/haproxy-manager",
  "description": "Set of console commands to manage an HAProxy instance through",
  "keywords": [
    "haproxy",
    "project",
    "console"
  ],
  "homepage": "https://github.com/merchnt/haproxy-manager",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "C:\\Development\\haproxy-client"
    }
  ],
  "require": {
    "merchnt/haproxy-api-client": "@dev",
    "symfony/console": "^4.3",
    "symfony/dotenv": "^4.3",
    "symfony/finder": "^4.3",
    "roave/better-reflection": "^3.5",
    "overtrue/phplint": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Johnathan Dell",
      "email": "open-source@merchnt.co"
    }
  ],
  "autoload": {
    "psr-4": {
      "Merchnt\\HAProxyManager\\": "src"
    },
    "classmap": [
      "app/Kernel.php"
    ],
    "files": [
      "helpers.php"
    ],
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Merchnt\\HAProxyManager\\Tests\\": "Tests/"
    }
  }
}
